datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
model User {
  id        String     @db.ObjectId 
  email     String    @id @map("_id")
  username  String   @unique
  password  String
  createdOn DateTime  @default(now())
  banned    Boolean   @default(false)
}
