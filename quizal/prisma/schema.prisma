datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @default(auto()) @map("_id") @db.ObjectId
  email     String    @id
  username  String    @unique
  password  String
  createdOn DateTime  @default(now())
  banned    Boolean   @default(false)
}
